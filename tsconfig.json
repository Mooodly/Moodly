{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": [
      "ES2020"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@processes/*": [
        "src/processes/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@widgets/*": [
        "src/widgets/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@entities/*": [
        "src/entities/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    },
    "types": [
      "nativewind/types",
      "react",
      "react-native",
      "expo"
    ]
  },
  "include": [
    "App.tsx",
    "index.tsx",
    "src",
    "global.d.ts",
    "global.css",
    "babel.config.js",
    "metro.config.js",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "android",
    "ios"
  ]
}